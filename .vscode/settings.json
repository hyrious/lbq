{
  "eslint.experimental.useFlatConfig": true,
  "prettier.enable": false,
  "eslint.enable": true,
  "cSpell.enabled": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "cSpell.words": [
    "sukka",
    "tildify"
  ]
}
